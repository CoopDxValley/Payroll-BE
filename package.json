{
  "name": "hello-prisma",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && pm2 start ecosystem.config.json",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "postinstall": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "seed:admin": "ts-node src/seeders/superadmin.ts",
    "worker": "cross-env NODE_ENV=development nodemon src/jobs/index.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "build": "rimraf build && tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "verbose": true
  },
  "dependencies": {
    "@bull-board/api": "^6.9.6",
    "@bull-board/express": "^6.9.6",
    "@prisma/client": "6.9.0",
    "@types/express": "^5.0.2",
    "@types/node": "22.15.31",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.53.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "16.5.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemon": "^3.1.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "xss": "^1.0.15",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.18",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "jest": "^30.0.4",
    "prisma": "6.9.0",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.2",
    "tsx": "^4.19.4",
    "typescript": "5.8.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
